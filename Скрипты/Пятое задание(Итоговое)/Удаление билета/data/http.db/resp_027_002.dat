.block_blog {position: relative; margin: 5px 0 0 0px; padding: 20px 0 15px 100px}
.block_blog * {list-style: none; margin: 0; padding: 0}
.blogs {height: 45px; overflow: hidden; position: relative; position: absolute; top: 18px; left: 0}
   .blogs ul {height: 26px; overflow: hidden; margin-bottom: 3px}
      .blogs li {float: left; padding-right: 4px}
      .blogs li a {background: url(//vkopt.net/forum/images/vkopt/icons/blogs.gif); width: 26px; height: 26px; float: left; display: block}
      .blogs .lj a {background-position: 0 0}         .blogs .lj a:hover {background-position: 0 -27px}
      .blogs .vk a {background-position: -27px 0}     .blogs .vk a:hover {background-position: -27px -27px}
      .blogs .fb a {background-position: -54px 0}     .blogs .fb a:hover {background-position: -54px -27px}
      .blogs .tw a {background-position: -81px 0}     .blogs .tw a:hover {background-position: -81px -27px}
      .blogs .ma a {background-position: -108px 0}    .blogs .ma a:hover {background-position: -108px -27px}
      .blogs .send a {background-position: -135px 0}  .blogs .send a:hover {background-position: -135px -27px}
      .blogs .ok a {background-position: -162px 0}    .blogs .ok a:hover {background-position: -162px -27px}
      .blogs .ya a {background-position: -189px 0}    .blogs .ya a:hover {background-position: -189px -27px}
   .blogs span {color: #777; font-size: 10px; white-space: nowrap; position: absolute}
   .block_blog .pls {background-position: 0 -12px}
.movie_send_alert .send_alert {margin-top: 0}
.note-classic,.note-important,.note-warning,.note-tip{margin:2em;margin-left:40px;margin-right:auto;width:70%!important;min-height:15px;clear:both;text-align:justify;vertical-align:middle;border-collapse:collapse;padding:15px 20px 15px 60px;background-position:20px 50%;background-repeat:no-repeat;-moz-border-radius:20px;-khtml-border-radius:20px;-webkit-border-radius:20px;border-radius:20px;}
.note-classic{background-color:#eef;}
.note-important{background-color:#ffc;}
.note-warning{background-color:#fdd;}
.note-tip{background-color:#dfd;}
.sitegpluse {
  margin-left:20px;
  opacity: 0.6;
  -webkit-filter: grayscale(100%);
  -ms-filter: grayscale(100%);   
  -moz-filter: grayscale(100%);
  -o-filter: grayscale(100%);
  filter: grayscale(100%);
}
.sitegpluse:hover {
  opacity: 1;
  -webkit-filter: grayscale(0%);
  -ms-filter: grayscale(0%);   
  -moz-filter: grayscale(0%);
  -o-filter: grayscale(0%);
  filter: grayscale(0%);
}